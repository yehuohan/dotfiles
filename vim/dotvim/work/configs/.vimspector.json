{
    "configurations": {
        "Launch rust,cxx with CodeLLDB": {
            "adapter": "CodeLLDB",
            "configuration": {
                "request": "launch",
                "cwd": "${workspaceRoot}",
                "program": "${workspaceRoot}/target/debug/main"
            }
        },
        "Launch cxx with cpptools": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "request": "launch",
                "cwd": "${workspaceRoot}",
                "program": "${workspaceRoot}/main"
            }
        },
        "Launch python with debugpy": {
            "adapter": "debugpy",
            "configuration": {
                "request": "launch",
                "type": "python",
                "cwd": "${workspaceRoot}",
                "program": "${workspaceRoot}/main.py",
                "stopOnEntry": true,
                "console": "integratedTerminal"
            }
        }

    }
}
