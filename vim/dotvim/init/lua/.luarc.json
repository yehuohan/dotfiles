{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "runtime.version": "LuaJIT",
    "workspace.library": [
        "./",
        "${env:VIMRUNTIME}/lua",
        "${env:VIMRUNTIME}/lua/vim",
        "${env:VIMRUNTIME}/lua/vim/lsp",
        "${env:VIMRUNTIME}/lua/vim/treesitter",
        "${env:DotVimDir}/bundle/plenary.nvim/lua"
    ],
    "diagnostics.disable": [
        "inject-field",
        "missing-fields",
        "undefined-field",
        "duplicate-set-field",
        "param-type-mismatch"
    ],
    "telemetry.enable": false,
    "format.enable": false
}

