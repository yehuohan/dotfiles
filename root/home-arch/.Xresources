!===============================================================================
! Xft
!===============================================================================
Xft.dpi: 115
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb


!===============================================================================
! xterm
!===============================================================================
xterm.termName: xterm-256color

! Dracula
*.background: #282A36
*.foreground: #F8F8F2
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! font
xterm*faceName: UbuntuMono Nerd Font Mono:antialias=True:pixelsize=20
xterm*faceNameDoublesize: WenQuanYi Micro Hei Mono:antialias=True:pixelsize=19
xterm*faceSize: 13
xterm*scaleHeight: 1.0

! option
xterm*loginshell: true
xterm*scrollBar: false
xterm*rightScrollBar: true
xterm*cursorBlink: true
xterm*geometry: 80x36

xterm.locale:true
xterm.utf8: true
xterm*utf8Title:true

! key
xterm*eightBitInput: false
xterm*altSendsEscape: true

! clipboard
xterm*selectToClipboard: true
xterm*translations: #override \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

